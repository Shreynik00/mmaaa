{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "/api/displaydata.js"
    },
    {
      "src": "/",
      "dest": "/api/displaydata.js"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
